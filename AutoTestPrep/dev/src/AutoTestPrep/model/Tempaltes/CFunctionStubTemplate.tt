<#@ template language="C#" #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="System.Text" #>
<#@ import namespace="System.Collections.Generic" #>
<#= Function.ToString() #>
{
<# if (!(("void").Equals(Function.DataType, StringComparison.Ordinal))) { #>
	<#= Function.DataType #> returnLatch = <#= Function.Name #>_return[<#= Function.Name #>];
<# } #>

<# foreach (var argument in Function.Arguments) { #>
	<#= Function.Name #>.<#= argument.Name #>[<#= Function.Name #>_called_count] = <#= argument.Name #>;
<# } #>

	<#= Function.Name #>_called_count++;

<# if (!(("void").Equals(Function.DataType, StringComparison.Ordinal))) { #>
	return returnLatch;
<# } #>
}
